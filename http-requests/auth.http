@host = http://localhost:3000
@contentType = application/json

###
# 사용자 목록 조회
GET {{host}}/users

###
# 사용자 상세 조회
GET {{host}}/users/1

###
# 로그인
POST {{host}}/auth/login
Content-Type: {{contentType}}
Accept: {{contentType}}

{
  "u_id": "사용자ID",
  "u_password": "비밀번호호호"
}

###
# 사용자 정보 업데이트
PUT {{host}}/users/1
Content-Type: {{contentType}}

{
  "u_email": "updated email"
}

###
# 사용자 삭제
DELETE {{host}}/users/1